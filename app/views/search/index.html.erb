<% @response.each do |event| %>
  <%= event[:title] %> // 
  <%= event[:city_name] %> // 
  <%= event[:start_time] %> // 
  <% if event[:description] %>
    <%= event[:description][0..140] %>
  <% else %>
    no description available
  <% end %> // 
  <%= event[:venue_name] %> // 
  <a href="<%= event[:venue_url] %>">Venue URL</a> // 
  <%= event[:venue_address] %>
  <br>
<% end %> 